#include <iostream>
void swap(int * ptr1, int * ptr2) {
	int num = *ptr1;//num이 ptr2가 가지리는 값 가짐
	*ptr1 = *ptr2;//ptr1에 ptr2이 가리키는 값 대입
	*ptr2 = num;//ptr2에 보관했던 ptr1값 대입
}

void swap(char* ptr1, char* ptr2) {
	char num = *ptr1;//num이 ptr2가 가지리는 값 가짐
	*ptr1 = *ptr2;//ptr1에 ptr2이 가리키는 값 대입
	*ptr2 = num;//ptr2에 보관했던 ptr1값 대입
}

void swap(double* ptr1, double* ptr2) {
	double num = *ptr1;//num이 ptr2가 가지리는 값 가짐
	*ptr1 = *ptr2;//ptr1에 ptr2이 가리키는 값 대입
	*ptr2 = num;//ptr2에 보관했던 ptr1값 대입
}

int main() {
	int num1 = 20, num2 = 30;
	swap(&num1, &num2);
	std::cout << num1 << ' ' << num2 << std::endl;

	char ch1 = 'A', ch2 = 'Z';
	swap(&ch1, &ch2);
	std::cout << ch1 << ' ' << ch2 << std::endl;

	double dbl1 = 1.111, dbl2 = 5.555;
	swap(&dbl1, &dbl2);
	std::cout << dbl1 << ' ' << dbl2 << std::endl;
	return 0;
}
